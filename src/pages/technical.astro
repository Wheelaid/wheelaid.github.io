---
import BaseHead from '../components/BaseHead.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';


import {Content} from '../content/pages/technical.md';
import * as params from '../content/pages/technical.md';

import BlogPost from '../layouts/BlogPost.astro';

---

<BlogPost 
    title={params.frontmatter.title}
    description={params.frontmatter.description}
    pubDate={new Date(params.frontmatter.pubDate)}
    heroImage={params.frontmatter.heroImage}>
        <Content/>
</BlogPost>
